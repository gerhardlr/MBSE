    <db:othercredit/>
    <db:pubdate fo:font-size="{$body.font.master * 1.44}pt" fo:space-before="2in"/>
  </fo:block>
</db:book>

<db:book t:side="verso">
  <fo:block font-family="{$title.font.family}">
    <db:title fo:font-weight="bold"/>
