    <db:pubdate fo:space-before="1em"/>
    <db:revision/>
    <db:revhistory/>
    <db:abstract/>
  </fo:block>
</db:book>

<db:book t:side="before-verso">
  <fo:block break-after="page"/>
</db:book>

